{
  "name": "@dext7r/puppeteer",
  "version": "0.0.1",
  "description": "A lightweight wrapper for Puppeteer, a powerful Chrome automation tool for Node.js.",
  "scripts": {
    "task": "npx ts-node ./src/serve",
    "build": "tsc",
    "precommit": "lint-staged",
    "prepush": "npm run lint",
    "prepare": "husky install",
    "commit": "git add . && cz",
    "lint": "tsc --noEmit && eslint src --ext .ts"
  },
  "keywords": [],
  "authors": [
    "h7ml <h7ml@qq.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "react": "17",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@easyv/linter": "0.1.5",
    "@types/node": "^18.16.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.0.0",
    "eslint": "8.2.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^1.3.6",
    "x-crawl": "^6.0.1"
  },
  "volta": {
    "node": "20.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint-config-airbnb",
        "eslint-plugin-jsx-a11y",
        "typescript"
      ]
    }
  }
}
